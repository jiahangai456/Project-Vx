<!--pages/contact/contact.wxml-->
<view class="divider">
    
</view>
<view class="cell">
    <form bindsubmit="submitHandle">
        <view class="cell-item ">
            <text>添加用户信息</text>
        </view>
        <view class="cell-item">
            <input type="text" placeholder="请输入账号..." name="uname" />
        </view>
        <view class="cell-item">
            <input type="password" placeholder="请输入密码..." name="upwd" />
        </view>
        <view class="cell-item">
            <radio-group>
                <radio name="r" checked value="male" />男
                <radio name="r" checked value="female" />女        
            </radio-group>     
        </view>
        <view>
            个人喜好：
        </view>
        <view class="cell-item">
            <checkbox-group>
                <checkbox checked>水果</checkbox>
                <checkbox >甜品</checkbox>
                <checkbox >蔬菜</checkbox>
                <checkbox >肉类</checkbox>
            </checkbox-group>
         </view>
        <view class="cell-item ">
            
                <button formType="submit" class="zcbt">一键注册</button>
        </view>
    </form>
</view>
<view class="divider">
</view>
<view class="cell">
    <navigator url="" redirect hover-class="className" class="lx">有不懂？联系我们</navigator>
</view>